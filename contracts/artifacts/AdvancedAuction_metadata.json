				"constructor": {
					"notice": "Auction constructor. Assigns the contract creator as the initial owner."
				},
				"endAuction()": {
					"notice": "Ends the auction manually or automatically. It can be called only by the contract owner or automatically after surpassing the end date."
				},
				"getBids()": {
					"notice": "Returns the list of bidders and their bid amounts."
				},
				"getWinner()": {
					"notice": "Returns the winner of the auction and the winning bid amount."
				},
				"placeBid(bool,uint256)": {
					"notice": "Allows participants to place a bid for the item. secret Indicates whether the bid is secret or not. amount The bid amount, if not secret."
				},
				"refund(uint256)": {
					"notice": "Allows participants to withdraw a percentage of their deposit before the end of the auction. _percentage The percentage of the deposit to refund."
				},
				"startAuction(uint256,uint256,uint256)": {
					"notice": "Starts a new auction with the specified parameters. initialPrice The initial price of the item. startDate The start date of the auction. duration The duration of the auction in seconds."
				},
				"withdrawDeposit()": {
					"notice": "Allows participants to withdraw their deposit before the end of the auction."
				}
